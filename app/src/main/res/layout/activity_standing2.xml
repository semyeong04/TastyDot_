<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF">

    <Button
        android:id="@+id/button14"
        android:layout_width="98dp"
        android:layout_height="48dp"
        android:backgroundTint="#FFFFFF"
        android:hint="나가기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="79dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="116dp"
        android:gravity="center"
        android:text="사업자정보"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.669"
        app:layout_constraintStart_toEndOf="@+id/button14"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="413dp"
        android:layout_height="609dp"
        android:background="@drawable/edge"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView31"
                android:layout_width="302dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="사업자등록증에 있는 정보를 그대로 옮겨주세요"
                android:textSize="25sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView32"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:hint="사업자 등록증" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="0dp"
                    app:srcCompat="@drawable/img" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:paddingLeft="10dp"
                android:text="사업자등록번호" />

            <EditText
                android:id="@+id/et5000"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="사업자등록번호를 입력해주세요."
                android:inputType="text"
                android:paddingLeft="10dp"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="과세구분" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="업태" />

            <EditText
                android:id="@+id/editTextText5"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="한글로만 입력해주세요."
                android:inputType="text"
                android:paddingLeft="10dp"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="호프,주점,도매업은 가입할 수 없어요."
                android:textColor="#565454"
                android:textColorHint="#000000"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="종목" />

            <EditText
                android:id="@+id/editTextText6"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="한글로만 입력해주세요."
                android:inputType="text"
                android:paddingLeft="10dp"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="영업신고증 고유번호" />

            <EditText
                android:id="@+id/editTextText7"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="제                                                                          호"
                android:inputType="text"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="영업신고증 가장 왼쪽, 제일 위에 있어요."
                android:textColor="#565454"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="영업의 종류" />

            <EditText
                android:id="@+id/editTextText8"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="예) 식품접객업"
                android:inputType="text"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="대표자" />

            <EditText
                android:id="@+id/editTextText9"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="대표자를 입력해주세요."
                android:inputType="text"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="영업소 명칭(상호명)" />

            <EditText
                android:id="@+id/editTextText10"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="상호명을 입력해주세요."
                android:inputType="text"
                android:paddingLeft="10dp" />

            <TextView
                android:id="@+id/textView37"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="소재지(가게 주소)" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/editTextText11"
                    android:layout_width="196dp"
                    android:layout_height="42dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/edge"
                    android:ems="10"
                    android:inputType="text" />

                <Button
                    android:layout_width="196dp"
                    android:layout_height="42dp"
                    android:layout_weight="1"
                    android:backgroundTint="#1B7CFE"
                    android:text="주소찾기"
                    android:textColor="#FFFFFF"
                    app:cornerRadius="10dp" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView38"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:text="상세주소" />

            <EditText
                android:id="@+id/editTextText12"
                android:layout_width="392dp"
                android:layout_height="42dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/edge"
                android:ems="10"
                android:hint="상세주소를 입력해주세요."
                android:inputType="text" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="match_parent"
                android:layout_height="42dp"
                android:backgroundTint="#1B7CFD" />

            <Button
                android:id="@+id/button8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#1B7CFD"
                android:text="다음" />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="match_parent"
                android:layout_height="42dp" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>